{
  "name": "ts-aspcore2-parser",
  "version": "1.0.1",
  "description": "An ASP-CORE-2 input language format parser implemented in TypeScript.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "npm run generate && tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "generate": "npm run antlr4ts && npm run antlr4ts_annotated",
    "antlr4ts": "antlr4ts -visitor ./src/grammar/AspCore2ILFGrammar.g4 -o ./src/parser",
    "antlr4ts_annotated": "antlr4ts -visitor ./src/grammar/AnnotatedAspCore2ILFGrammar.g4 -o ./src/parser"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tobiasberei/ts-aspcore2-parser.git"
  },
  "keywords": [
    "answer",
    "set",
    "programming",
    "parser",
    "typescript"
  ],
  "author": "Tobias Berei",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/tobiasberei/ts-aspcore2-parser/issues"
  },
  "homepage": "https://github.com/tobiasberei/ts-aspcore2-parser#readme",
  "devDependencies": {
    "@types/jest": "^24.0.20",
    "antlr4ts-cli": "^0.5.0-alpha.3",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "antlr4ts": "^0.5.0-alpha.3"
  },
  "files": [
    "lib/*",
    "lib/parser/*"
  ]
}
